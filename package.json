{
  "name": "pokeapiclient",
  "version": "1.0.0",
  "description": "Simple client for pokeapi",
  "main": "main.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --watch --config webpack.config",
    "test": "jest --verbose",
    "start": "electron .",
    "dist": "build",
    "dist-mac": "build --mac",
    "dist-linux": "build --linux --ia32 --x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BlackHarpy/pokeapi-client.git"
  },
  "author": "Maria C",
  "license": "ISC",
  "email": "mariacastro87@gmail.com",
  "bugs": {
    "url": "https://github.com/BlackHarpy/pokeapi-client/issues"
  },
  "homepage": "https://github.com/BlackHarpy/pokeapi-client#readme",
  "dependencies": {
    "animejs": "^2.2.0",
    "material-ui": "^0.20.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-md-spinner": "^0.2.5",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "electron": "^1.8.2",
    "electron-builder": "^20.0.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "fetch-mock": "^6.0.0",
    "jest": "^22.0.4",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.5.1",
    "webpack": "^3.10.0"
  },
  "build": {
    "appId": "pokeapi.client",
    "icon": "build/icon.256x256.png",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": "portable",
      "icon": "build/icon.256x256.png"
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/icon.256x256.png"
    }
  }
}
